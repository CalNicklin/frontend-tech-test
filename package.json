    "test": "vitest"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vercel/style-guide": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.0",
    "globals": "^15.14.0",
    "prettier": "3.4.2",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "prettier": "@vercel/style-guide/prettier",
  "packageManager": "pnpm@9.4.0"
