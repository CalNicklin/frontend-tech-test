{
  "name": "tech-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm run dev:client & pnpm run dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch server/src/index.ts",
    "build": "tsc -b && vite build",
    "build:server": "tsc -p tsconfig.server.json",
